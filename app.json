{
  "expo": {
    "name": "example-splash-screen",
    "slug": "example-splash-screen",
    "orientation": "landscape",
    "icon": "./assets/AppIcon.png",
    "android": {
      "package": "com.whiteroseeducation.examplesplashscreen"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#002E63",
          "image": "./assets/AppIcon.png",
          "imageWidth": 180
        }
      ],
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "parentTheme": "Light",
            "enforceNavigationBarContrast": false
          }
        }
      ]
    ]
  }
}
